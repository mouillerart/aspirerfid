<?xml version="1.0" encoding="UTF-8"?>
<component name="zigbee.cmd">
  <!-- xmlns="http://www.osgi.org/xmlns/scr/v1.0.0" -->
	<implementation class="org.ow2.aspirerfid.sensor.zigbee.device.cmd.ZigBeeCmdImpl"/>
	
	<service>
		<provide interface="org.apache.felix.shell.Command"/>
	</service>

	<reference name="NODEDISCOVERY"
	    interface="org.ow2.aspirerfid.sensor.zigbee.device.NodeDiscoveryService"
	    cardinality="1..1"
	    policy="dynamic"
	    bind="bindNodeDiscoveryService"
	    unbind="unbindNodeDiscoveryService"
	/>
	
	<reference name="NODEPACKET"
	    interface="org.ow2.aspirerfid.sensor.zigbee.device.NodePacketService"
	    cardinality="1..1"
	    policy="dynamic"
	    bind="bindNodePacketService"
	    unbind="unbindNodePacketService"
	/>
	
</component>
